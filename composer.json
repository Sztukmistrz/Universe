{
    "name": "Sztukmistrz/Universe",
    "type": "library",
    "description": "Laravel5 syetem",
    "keywords": [
        "Sztukmistrz",
        "Universe"
    ],
    "homepage": "https://github.com/Sztukmistrz/Universe",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Pu≈Çtorak",
            "email": "admin@krzyszttofpultotak.pl",
            "homepage": "https://github.com/Sztukmistrz/Universe/wiki",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Sztukmistrz/Pagina.git"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "Sztukmistrz/Pagina": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Sztukmistrz\\Universe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sztukmistrz\\Universe\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.1-dev"
        }
    },
    "minimum-stability": "stable"
}
